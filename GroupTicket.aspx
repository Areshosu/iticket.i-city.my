        <script type="text/javascript">
            $(function () {
                $("#toolTipTC").tooltipster({ position: 'top', interactive: true });
                if ($("#MainContent_repMisc")) {
                    CalculateMiscTotal();
                }
            });

            function CalculateMiscTotal() {
                var subtotal = 0;

                $("select[id*=ddlQuantity]").each(function () {
                    var quantity = parseInt($(this).val());
                    var amt = parseFloat($(this).parent().next().html().replace("RM ", "").replace(",", ""));

                    $(this).parent().next().next().html("RM " + parseFloat(Math.round(quantity * amt * 100) / 100).toFixed(2));
                    subtotal = subtotal + parseFloat(Math.round(quantity * amt * 100) / 100).toFixed(2) * 1.0;
                });

                $("#MainContent_litMiscOverallTotal").html("RM " + parseFloat(subtotal).toFixed(2));
                $("#MainContent_litGrandTotal").html("RM " + (parseFloat($("#MainContent_litOverallTotal").html().replace("RM ", "").replace(",", "")) + subtotal).toFixed(2));
            }

            function CheckTC() {
                var isSelected = document.getElementById("MainContent_chkAgree").checked;

                if (isSelected == false) {
                    $("#toolTipTC").tooltipster('show');
                }

                return isSelected;
            }

            $('#MainContent_btnAgreeProceed').dblclick(function (e) {
                e.preventDefault();
            })

